{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.language": "en-GB",
  "typescript.tsdk": "node_modules/typescript/lib",
  "snyk.advanced.additionalParameters": "--all-projects",
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "css.customData": [".vscode/tailwind.json"],
  "eslint.validate": ["javascript", "typescript", "vue"],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.nx/*/**": true,
    "**/.turbo/*/**": true,
    "**/.coverage/*/**": true,
    "**/libs/*/**": true,
    "**/dist/*/**": true
  },
  "cSpell.ignorePaths": [
    "package-lock.json",
    "package.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders"
  ],
  "cSpell.words": [
